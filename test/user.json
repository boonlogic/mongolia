{
  "$id": "https://gitlab.com/boonlogic/development/mongolia/mongolia/test/user.json",
  "type": "object",
  "required": [
    "userId",
    "username"
  ],
  "x-unique": [
    "userId"
  ],
  "properties": {
    "userId": {
      "type": "string",
      "pattern": "^[0-9a-z]{24}$"
    },
    "username": {
      "type": "string",
      "pattern": "^[a-zA-Z0-9-_]*$",
      "maxLength": 64
    }
  }
}
