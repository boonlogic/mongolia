{
  "$id": "https://gitlab.com/boonlogic/development/expert-api/api/amberv2/schemas/license.json",
  "type": "object",
  "required": [
    "_id",
    "licenseId",
    "name",
    "tenant",
    "models"
  ],
  "properties": {
    "_id": {
      "type": "objectId"
    },
    "licenseId": {
      "type": "string",
      "format": "uuid"
    },
    "name": {
      "type": "string",
      "pattern": "^[a-zA-Z0-9-_]*$",
      "maxLength": 64
    },
    "tenant": {
      "type": "objectId",
      "$ref": "/schemas/tenant.json"
    },
    "models": {
      "type": "array",
      "item": {
        "type": "objectId",
        "$ref": "/schemas/model.json"
      },
      "default": []
    }
  },
  "additionalProperties": false
}
